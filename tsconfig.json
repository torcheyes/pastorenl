{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@public/*": [
        "./public/*"
      ],
      "@middleware/*": [
        "./src/middleware/*"
      ],
      "@controllers/*": [
        "./src/controllers/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@containers/*": [
        "./src/containers/*"
      ],
      "@models/*": [
        "./src/models/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@types": [
        "./src/types/types.d.ts"
      ],
      "@utils/*": [
        "./src/utils/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/svg/Social/whatsapp.tsx",
    "build/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
